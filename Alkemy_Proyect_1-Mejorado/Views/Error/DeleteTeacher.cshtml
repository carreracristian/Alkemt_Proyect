{
    ViewBag.Title = "DELETE TEACHER ERROR";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">
    <h1>
        Error, CANT DELETE THE TEACHER
    </h1>
</section>
<section class="content">
    <div class="error-page">
        <h2 class="headline text-red">Error</h2>
        <div class="error-content">
            <h3><i class="fa fa-warning text-red"></i>Sin autorización</h3>
            @if (ViewBag.operacion != "")
            {
                <p>Operación @ViewBag.operacion </p>
            }
        <a href="~/Views/Error/DeleteTeacher.cshtml">~/Views/Error/DeleteTeacher.cshtml</a>
            @if (ViewBag.modulo != "")
            {
                <p>Modulo @ViewBag.modulo</p>
            }
            @if (ViewBag.msjeErrorExcepcion != "")
            {
                <p>@ViewBag.msjeErrorExcepcion</p>
            }
        </div>
    </div>
</section>